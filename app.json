{
  "expo": {
    "name": "FridgePal",
    "slug": "FridgePal",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "fridgepal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.catalina-ava.FridgePal"
    },
    "icon": "./assets/images/splash-icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["android.permission.CAMERA"],
      "package": "com.catalina_ava.FridgePal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FridgePal to access your camera",
          "microphonePermission": "Allow FridgePal to access your microphone",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [],
          "color": "#4F46E5"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f14aa93e-7e55-4f85-8073-7b09b2e47e53"
      }
    }
  }
}
