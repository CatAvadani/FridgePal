{
  "expo": {
    "name": "FridgePal",
    "slug": "FridgePal",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/images/icon.png",
    "scheme": "fridgepal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FridgePal to access your camera",
          "microphonePermission": "Allow FridgePal to access your microphone",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [],
          "color": "#4F46E5"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
